    "for i,v in enumerate(hashCounts.Prediction.unique()):\n",
    "    df = hashCounts[hashCounts.Prediction == v]\n",
    "    df = df.groupby(['word']).sum().sort_values(by='number', ascending=False)\n",
    "    Predic = 'Predict_Hashtags_By_Community_' + str(v) + '.csv'\n",
    "    df.to_csv(Predic)\n",
