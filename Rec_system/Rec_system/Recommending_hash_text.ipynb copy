   "cell_type": "code",
   "execution_count": 19,
   "metadata": {
    "collapsed": true
   },
   "outputs": [],
   "source": [
    "## get hashtags\n",
    "hashtags = []\n",
    "for i,v in enumerate(df.Caption):\n",
    "        v = str(v)\n",
    "        #tokenizedTweet = list(re.findall(r'(?i)\\#\\w+', str))\n",
    "        \n",
    "        hashtag = []\n",
    "        for t in v.split():\n",
    "            if '#' in t:\n",
    "                hashtag = hashtag + ['#' + x for x in t.split('#')[1:]]\n",
    "        #hashtag  =[ t for t in v.split() if '#' in t ]\n",
    "        #print(hashtag)\n",
    "        hashtags.append(' '.join(hashtag))\n",
    "        \n",
    "df['hashtags'] = pd.Series(hashtags)"
   ]
  },
